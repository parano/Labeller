<% if @label[0] == 'create' %>
  $("td.rawdata").each(function(){
    if($(this).text().indexOf("<%= @label[1] %>") > -1){
      var id = $(this).attr("id").substring(4);
      if ($("#label" + id + "> span").text().indexOf('unknown') > -1 ){
        $("#label" + id + "> span").text("<%= @label[1] %>").addClass("label-success");
        $("#button" + id).text("undo").addClass("btn-warning");
      }
    }
  });
<% else %>
  $("td.label_solution").each(function(){
    if($(this).text().indexOf('<%= @label[1] %>') > -1){
      var id = $(this).attr("id").substring(5);
      $("#label" + id + "> span").text("unknown").removeClass("label-success");
      $("#button" + id).text("label").removeClass("btn-warning");
    }
  });
<% end %>
