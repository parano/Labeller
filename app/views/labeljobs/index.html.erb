<h1>Listing labeljobs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Deadline</th>
    <th>Labels</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @labeljobs.each do |labeljob| %>
  <tr>
    <td><%= labeljob.name %></td>
    <td><%= labeljob.desc %></td>
    <td><%= labeljob.deadline %></td>
    <td><%= labeljob.labels %></td>
    <td><%= link_to 'Show', labeljob %></td>
    <td><%= link_to 'Edit', edit_labeljob_path(labeljob) %></td>
    <td><%= link_to 'Destroy', labeljob, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Labeljob', new_labeljob_path %>
